<?xml version='1.0' encoding='UTF-8' ?>
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:b="http://bootsfaces.net/ui"
	template="../plantillas/admin-lte.xhtml">

	<ui:define name="uiTituloVista">#{contenidoMiProyecto.tituloMiProyecto}</ui:define>

	<ui:define name="uiBreadCrumb">
		<ol class="breadcrumb">
			<li><a href="#{request.contextPath}/index.xhtml">Inicio</a></li>
			<li class="active">Mi Proyecto</li>
		</ol>
	</ui:define>
	
	<ui:define name="uiContenido">
		<div class="row">
			<div class="col-md-8 col-lg-7 top-media">
				<div class="embed-responsive embed-responsive-16by9">
					<iframe class="embed-responsive-item" src="https://www.youtube.com/embed/#{contenidoMiProyecto.archivo.youtube}?rel=0"
						allowfullscreen="allowfullscreen"></iframe>
				</div>
			</div>
				
			<div class="col-md-4 col-lg-5 top-media">
				<h:outputText escape="false" value="#{contenidoMiProyecto.enlaces}" />
			</div>
		</div>
		
		<ui:include src="../plantillas/publicidad.xhtml" />
	</ui:define>
	
</ui:composition>